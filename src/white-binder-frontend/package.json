{
  "name": "white-binder-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx canister create white-binder-backend && dfx generate white-binder-backend && dfx deploy",
    "start": "vite --port 3000",
    "prebuild": "dfx generate",
    "build": "tsc && vite build",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\"",
    "watch": "npx @tailwindcss/cli -i ./src/index.css -o ./src/output.css --watch"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "dotenv": "^16.3.1",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "typescript": "^5.9.2",
    "vite": "^4.3.9",
    "vite-plugin-environment": "^1.1.3"
  },
  "dependencies": {
    "@dfinity/agent": "^2.1.3",
    "@dfinity/candid": "^2.1.3",
    "@dfinity/principal": "^2.1.3",
    "@fortawesome/fontawesome-free": "^7.0.0",
    "@tailwindcss/cli": "^4.1.12",
    "chart.js": "^4.5.0",
    "pinia": "^2.1.6",
    "tailwindcss": "^4.1.12",
    "vue": "^3.3.4",
    "vue-router": "^4.5.1"
  }
}
